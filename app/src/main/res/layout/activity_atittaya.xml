<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".AtittayaActivity">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="185dp"
        android:layout_height="167dp"
        app:srcCompat="@drawable/w"
        tools:layout_editor_absoluteX="113dp"
        tools:layout_editor_absoluteY="77dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="นางสาวอาทิตยา ฉิมมาแก้ว"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="73dp"
        tools:layout_editor_absoluteY="274dp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="603410321-2"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="135dp"
        tools:layout_editor_absoluteY="317dp" />

    <SeekBar
        android:id="@+id/seekBar"
        android:layout_width="260dp"
        android:layout_height="84dp"
        android:layout_marginStart="7dp"
        android:layout_marginEnd="13dp"
        app:layout_constraintEnd_toStartOf="@+id/textView8"
        app:layout_constraintHorizontal_bias="0.545"
        app:layout_constraintStart_toEndOf="@+id/textView7"
        tools:layout_editor_absoluteY="377dp" />

    <Button
        android:id="@+id/button_savew"
        android:layout_width="219dp"
        android:layout_height="37dp"
        android:layout_marginStart="95dp"
        android:layout_marginEnd="97dp"
        android:background="@drawable/shape_pink_buttom"
        android:drawableBottom="@android:color/holo_red_light"
        android:foregroundTint="#E91E63"
        android:text="save"
        android:textColor="#0C0C0C"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="629dp" />

    <EditText
        android:id="@+id/editText2"
        android:layout_width="264dp"
        android:layout_height="90dp"
        android:layout_marginStart="71dp"
        android:layout_marginEnd="76dp"
        android:ems="10"
        android:gravity="start|top"
        android:inputType="textMultiLine"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="518dp" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Comment"
        tools:layout_editor_absoluteX="71dp"
        tools:layout_editor_absoluteY="485dp" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="23dp"
        android:layout_height="29dp"
        android:text="-5"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="31dp"
        tools:layout_editor_absoluteY="402dp" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="5"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="356dp"
        tools:layout_editor_absoluteY="399dp" />

</androidx.constraintlayout.widget.ConstraintLayout>